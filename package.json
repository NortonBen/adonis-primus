{
  "name": "@adonis/primus",
  "version": "0.0.1",
  "description": "Primus provider for Adonis 4.x",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha --report lcovonly test/unit/*.spec.js && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "istanbul cover _mocha test/unit/*.spec.js",
    "lint": "standard"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "websockets",
    "primus",
    "uws"
  ],
  "author": "norton",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.5",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cz-conventional-changelog": "^2.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^4.0.2",
    "socket.io-client": "^1.5.1",
    "standard": "^8.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "global": [
      "it",
      "use",
      "should",
      "describe",
      "beforeEach"
    ]
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-compose": "^3.0.0",
    "es6-class-mixin": "^1.0.5",
    "haye": "^1.0.1",
    "lodash": "^4.17.2",
    "macroable": "^1.0.0",
    "node-exceptions": "^1.0.3",
    "primus": "^7.1.0",
    "primus-emitter": "^3.1.1",
    "resetable": "^1.0.1",
    "uws": "^9.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NortonPerson/adonis-primus.git"
  },
  "bugs": {
    "url": "https://github.com/NortonPerson/adonis-primus/issues"
  },
  "homepage": "https://github.com/NortonPerson/adonis-primus#readme"
}
